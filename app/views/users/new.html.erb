<div class='authentication'>
  <p>No authentication information is available!</p>
  <p>Try one of the below Log In Method</p>
  </div>
  
<div class=login>

<h1>SIGN UP PAGE</h1>

<%= form_for @user do |f| %>
<%= f.label :NAME %>
<%= f.text_field :name %>
<%= f.label :Email %>
<%= f.text_field :email %>
<%= f.label :Password %>
<%= f.password_field :password %>
<%= f.label :Password_Confirmation %>
<%= f.password_field :password_confirmation %>

<%= f.submit 'Sign Up' %>
<% end %>

</div>

